    <script>
        var w = document.documentElement.clientWidth;
        var h = document.documentElement.clientHeight;
        var l = (w * 0.45) + "px";
        var l1 = (w * 0.225) + "px";
        var l2 = (w * 0.349) + "px";
        var l3 = (w * 0.1745) + "px";
        $(".tx").css({
            "width": l
        });
        $(".tx").css({
            "height": l
        });
        $(".tx").css({
            "border-radius": l1
        });
        $(".main7_1").css({
            "width": l2
        });
        $(".main7_1").css({
            "height": l2
        });
        $(".main7_1").css({
            "border-radius": l3
        });
        var txxw = $(".txx").width();
        var txxw1 = (txxw / 2) + "px";
        $(".txx").css({
            "height": txxw
        });
        $(".txx").css({
            "border-radius": txxw1
        });
        $(".mc").css({
            "line-height": txxw + "px"
        });
        $(".mc_name").css({
            "line-height": txxw + "px"
        });
        $(".mc_name").css({
            "height": txxw + "px"
        });
        $(".syl").css({
            "line-height": txxw + "px"
        });
        var myChart = echarts.init(document.getElementById('echart1'));

        function drawPic(arrDemo, data2) {
            var option = {
                tooltip: {
                    trigger: 'axis',
                    formatter: function(params) {
                        var valueName2 = '收益';
                        var value2 = params[1].value;
                        /*if (value1 && value2) {*/
                        return name + "<br/>" + valueName2 + ":" + value2;
                        /* }*/
                    },
                    backgroundColor: 'rgba(0, 0, 0, 0.3)'

                },
                xAxis: {
                    type: 'category',
                    boundaryGap: false,
                    /*坐标轴两边留白策略*/
                    data: arrDemo,
                    axisLine: {
                        lineStyle: {
                            color: '#8392A5'
                        }
                    },
                    /*坐标轴轴线*/
                    splitLine: {
                        /*坐标轴在grid中的分隔线*/
                        show: true,
                        lineStyle: {
                            color: '#f6f6f6',
                            type: 'dotted'
                        }
                    },
                    axisLabel: {
                        show: true,
                        interval: 0
                    },
                    axisTick: {
                        show: true
                    }
                },
                yAxis: {
                    type: 'value',
                    scale: 'true',
                    axisTick: {
                        show: false
                    },
                    axisLabel: {
                        formatter: '{value}%'
                    },
                    splitNumber: 2,
                    axisLine: {
                        lineStyle: {
                            color: '#8392A5'
                        }
                    },
                },
                animation: false,
                series: [{
                    /*折线图*/
                    type: 'line',
                    smooth: true,
                    data: data2,
                    symbol: 'none',
                    areaStyle: {
                        normal: {
                            color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{
                                offset: 0,
                                color: 'rgb(255, 158, 68)'
                            }, {
                                offset: 1,
                                color: 'rgba(255, 255, 255,0)'
                            }])
                        }
                    },
                    itemStyle: {
                        normal: {
                            color: '#FF1F64',
                            lineStyle: {
                                color: 'rgba(255, 158, 68,0)'
                            }
                        }
                    }
                }]
            };

            var xCenter = parseInt(arrDemo.length / 2);
            option.xAxis.splitLine.interval = function(index, value) {

                if (value == arrDemo[0] || value == arrDemo[arrDemo.length - 1] || value == arrDemo[xCenter]) {
                    return true;
                } else return false;
            }
            option.xAxis.axisTick.interval = function(index, value) {
                if (value == arrDemo[0] || value == arrDemo[arrDemo.length - 1] || value == arrDemo[xCenter]) {
                    return true;
                } else return false;
            }
            option.xAxis.axisLabel.formatter = function(value, index) {
                if (value == arrDemo[0] || value == arrDemo[arrDemo.length - 1] || value == arrDemo[xCenter]) {
                    return value.substring(4, 6) + "/" + value.substring(6, 8);
                }
            };
            //window.onresize = myChart.resize();

            return option
        }
        myChart.setOption(drawPic(@(JSON.stringify(locals.ProfitDaily.map(d => d.date))), @(JSON.stringify(locals.ProfitDaily.map(d => d.profit)))));
        window.onresize = myChart.resize()


        //myChart2.setOption(drawPic(@(JSON.stringify(locals.TeamProfitDaily.map(d=>d.date))), @(JSON.stringify(locals.TeamProfitDaily.map(d=>d.profit)))));
    </script>
    @if(locals.TeamProfitDaily) {
    <script>
        var myChart2 = echarts.init(document.getElementById('echart2'));
        myChart2.setOption(drawPic(@(JSON.stringify(locals.TeamProfitDaily.map(d => d.date))), @(JSON.stringify(locals.TeamProfitDaily.map(d => d.profit)))));
    </script>
    }

</body>

